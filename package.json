{
  "name": "idempotency-kit",
  "version": "0.1.0",
  "description": "Idempotency toolkit for Node.js and TypeScript with in-flight request deduplication and TTL response caching.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "bench": "vitest bench --run",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "idempotency",
    "idempotent",
    "idempotency-key",
    "request-deduplication",
    "promise-deduplication",
    "response-cache",
    "in-memory-cache",
    "deduplication",
    "in-flight",
    "nodejs",
    "api",
    "ttl",
    "memoization",
    "typescript",
    "node"
  ],
  "author": {
    "name": "Breno Quintela",
    "url": "https://github.com/GustavoQnt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GustavoQnt/idempotency-kit.git"
  },
  "homepage": "https://github.com/GustavoQnt/idempotency-kit#readme",
  "bugs": {
    "url": "https://github.com/GustavoQnt/idempotency-kit/issues"
  },
  "dependencies": {
    "@gustavoqnt/ttl-cache": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.8",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  }
}
